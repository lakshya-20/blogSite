<!DOCTYPE html>
<html lang="en">
    <head>    
        <title>Dashboard</title>
        <meta charset="UTF-8">
        <link rel='stylesheet' href='/stylesheets/dashboard.css' />
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta http-equiv="X-UA-Compatible" content="ie=edge">
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm"
    crossorigin="anonymous">
    </head>
    
    <body>
        <h1 id="heading">Dashboard</h1>
        <% include partials/publicNav.ejs %>
        
        
        <div id="body">
            <ul id="blogs" style="list-style-type:decimal;">
            <% files.forEach(function(file){%>
                <div id="blog">
                <li>
                <span id="title"><%=file.title%></span>
                <br>
                <span id="header"><%=file.header%></span>
                
                <span id="author">Author <%=file.author%></span>
                <br>
                <br>
                <span id="body"><%=file.body%></span>
                <br>
                
                </li>
                </div>
            <%})%>
        </ul>
        </div>
     
    </body>
</html>